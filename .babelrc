{
  "presets": [
    "es2015",
    "react",
    "babel-preset-lotteriz"
  ],
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ],
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }
          ]
        }]
      ]
    }
  }
}
